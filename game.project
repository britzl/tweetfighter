launch_image_1242x2208 = /assets/loadingscreens/screen_1242x2208.png
launch_image_2208x1242 = /assets/loadingscreens/screen_2208x1242.png
launch_image_2048x1536 = /assets/loadingscreens/screen_2048x1536.png
