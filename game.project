[project]
title = TweetFighter
dependencies = https://github.com/britzl/monarch/archive/1.1.zip,https://github.com/britzl/ludobits/archive/1.3.5.zip,https://github.com/britzl/defold-timer/archive/1.1.1.zip,https://github.com/britzl/defold-googleanalytics/archive/1.3.2.zip

[bootstrap]
main_collection = /controller/controller.collectionc
render = /builtins/render/default.renderc

[input]
game_binding = /input/game.input_bindingc

[display]
width = 640
height = 1136
variable_dt = 1

[script]
shared_state = 1

[ios]
app_icon_120x120 = /assets/appicons/Icon-App-60x60@2x.png
app_icon_180x180 = /assets/appicons/Icon-App-60x60@3x.png
